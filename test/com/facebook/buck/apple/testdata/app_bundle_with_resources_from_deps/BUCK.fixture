apple_bundle(
    name = "bundle",
    binary = ":binary",
    extension = "app",
    info_plist = "Info.plist",
)

apple_binary(
    name = "binary",
    srcs = ["main.m"],
    deps = [
        ":lib_with_resource",
    ],
)

apple_library(
    name = "lib_with_resource",
    deps = [
        ":resource",
    ],
)

apple_resource(
    name = "resource",
    resources_from_deps = [
        ":other_lib_with_resource",
    ],
)

apple_library(
    name = "other_lib_with_resource",
    deps = [
        ":other_resource",
    ],
)

apple_resource(
    name = "other_resource",
    files = ["other_resource.txt"],
)
